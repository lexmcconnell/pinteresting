<div id="pins" class="transitions-enabled">
          <% @pins.each do |pin| %>
          <div class='box panel panel-defult'>
              <%= link_to image_tag pin.image.url :medium  %>
              <div class="panel-body">
                <%= link_to pin.description, pin %>
                <%= pin.user.email if pin.user %>
                <% if pin.user == current_user %>
                <%= link_to 'Edit', edit_pin_path(pin), class: 'redtext' %>
                <%= link_to 'Delete', pin, method: :delete, data: { confirm: 'Are you sure?' }, class: 'redtext' %>
                <% end %>
              </div>
            </div>
            <% end %>           
</div>
